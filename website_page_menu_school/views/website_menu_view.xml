<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="website_menu_school_view_form" model="ir.ui.view">
        <field name="model">website.menu</field>
        <field name="inherit_id" ref="website.website_menus_form_view" />
        <field name="arch" type="xml">
            <field name="sequence" position="after">
                <field name="school_id" />
            </field>
        </field>
    </record>

    <record id="website_menu_school_view_tree" model="ir.ui.view">
        <field name="model">website.menu</field>
        <field name="inherit_id" ref="website.menu_tree" />
        <field name="arch" type="xml">
            <field name="new_window" position="after">
                <field name="school_id" />
            </field>
        </field>
    </record>
</odoo>
