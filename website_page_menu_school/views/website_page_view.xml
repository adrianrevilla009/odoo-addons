<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="website_page_school_view_form" model="ir.ui.view">
        <field name="model">website.page</field>
        <field name="inherit_id" ref="website.website_pages_form_view" />
        <field name="arch" type="xml">
            <field name="is_published" position="after">
                <field name="school_id" />
            </field>
        </field>
    </record>

    <record id="website_page_school_view_tree" model="ir.ui.view">
        <field name="model">website.page</field>
        <field name="inherit_id" ref="website.website_pages_tree_view" />
        <field name="arch" type="xml">
            <field name="website_published" position="after">
                <field name="school_id" />
            </field>
        </field>
    </record>
</odoo>
